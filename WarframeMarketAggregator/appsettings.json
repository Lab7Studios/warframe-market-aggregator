{
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "ServiceSettings": {
    "WarframeMarketBaseUrl": "https://api.warframe.market/v1",
    "GlobalLogger": {
      "RollingFileSink": {
        "Enabled":  false, 
        "LogFilePath": "/logs/",
        "LogFileNameTemplate": "warframe-market-aggregator_{0}.txt",
        "RetainedFileCountLimit":  10 
      } 
    } 
  } 
}
